def student_grouper(students):
  
  aboveAveStudents = []
  aveStudents = []
  belowAveStudents = []
  for index, row in newdf.iterrows():
    
    #tempdData = pd.DataFrame()
    #tempData = newdf.loc[newdf['Student'] == s]
    
    if(row['GPA'].values>=3.5):
      
      aboveAveStudents.append(row.values)
    elif(row['GPA'].values<3.5 and row['GPA'].values>2.5):
      aveStudents.append(row.values)
    elif(row['GPA'].values<2.5):
      belowAveStudents.append(row.values)
                              
                    
  return pd.DataFrame(aboveAveStudents),pd.DataFrame(aveStudents),pd.DataFrame(belowAveStudents)

