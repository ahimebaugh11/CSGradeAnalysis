def create_grades(meanHigh, meanLow, varianceMean, varianceSTD,studentCount):
  for s in range(studentCount):
    semesters, grades = gen_grades(float(np.random.randint(meanLow,meanHigh)/100), np.random.normal(varianceMean, varianceSTD))
    a = semesters
    b = grades
    
  return a,b

